{{#each books as |book|}}

 <!-- Developers, why are we using data-id and not just id? -->
<div id="book-listing" data-id="{{book.id}}">
  <p>Book ID: {{book.id}}</p>
  <p>Title: {{book.title}}</p>
  <p>{{book.description}}</p>
  <p><button class="remove-button">Remove Book</button></p>
</div>
{{/each}}
